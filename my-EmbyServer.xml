<?xml version="1.0"?>
<Container version="2">
  <Name>EmbyServer</Name>
  <Repository>zishuo/embyserver</Repository>
  <Registry>https://registry.hub.docker.com/r/emby/embyserver/</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://lime-technology.com/forum/index.php?topic=45444.0</Support>
  <Project/>
  <Overview>Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.</Overview>
  <Category>MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
  <WebUI>http://[IP]:[PORT:8096]/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/MediaBrowser/Emby.Build/master/unraid-templates/emby/EmbyServer.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/MediaBrowser/Emby.Resources/master/images/Logos/logoicon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1611103497</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>Emby Server is a home media server built on top of other popular open source technologies such as Service Stack, jQuery, jQuery mobile, and Mono. Bringing together movies, music, television and more in one dazzling interface.</Description>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>8086</HostPort>
        <ContainerPort>8086</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Movie/</HostDir>
      <ContainerDir>/mnt</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/EmbyServer</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>UID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>200</Value>
      <Name>GID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>2</Value>
      <Name>GIDLIST</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Port 1" Target="8086" Default="8096" Mode="tcp" Description="n/a" Type="Port" Display="always" Required="true" Mask="false">8086</Config>
  <Config Name="Host Path 2" Target="/mnt" Default="/mnt" Mode="rw" Description="Container Path: /mnt" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/Movie/</Config>
  <Config Name="Key 1" Target="UID" Default="2" Mode="" Description="Container Variable: UID" Type="Variable" Display="always" Required="false" Mask="false">99</Config>
  <Config Name="Key 2" Target="GID" Default="2" Mode="" Description="Container Variable: GID" Type="Variable" Display="always" Required="false" Mask="false">200</Config>
  <Config Name="Key 3" Target="GIDLIST" Default="2" Mode="" Description="Container Variable: GIDLIST" Type="Variable" Display="always" Required="false" Mask="false">2</Config>
  <Config Name="AppData &#x914D;&#x7F6E;&#x8DEF;&#x5F84;" Target="/config" Default="/mnt/user/appdata/EmbyServer" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/appdata/EmbyServer</Config>
</Container>
